{{#each this}}
<h2 class="country-name">{{name}}</h2>
<div class="country-wrap">
    <div>
    <p class="country-item"><span class="country-characters">Capital: </span>{{capital}}</p>
    <p class="country-item"><span class="country-characters">Population: </span>{{population}}</p>
    <ul class="country-item"><span class="country-characters">Languages: </span>{{#each languages}}<li>{{name}}</li>{{/each}}</ul>
    </div>
    <img src='{{flag}}' alt="Flag of {{name}}" class="country-img">
</div>
{{/each}}